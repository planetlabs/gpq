{
  "name": "gpq",
  "private": true,
  "scripts": {
    "test": "eslint main.js"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "eslint-config-planet": "^20.0.3",
    "vite": "^5.0.0"
  },
  "eslintConfig": {
    "extends": "planet",
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "ignorePatterns": [
      "wasm_exec.js"
    ],
    "globals": {
      "Go": "readonly"
    },
    "rules": {
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "^https?://"
          ]
        }
      ]
    }
  }
}
